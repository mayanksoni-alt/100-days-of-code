#include <stdio.h>
int main() {
   int l1, l2, l3;
  
        printf("Enter the lenghts of the sides of the triangle: ");
        scanf("%d%d%d", &l1, &l2, &l3);
  if(l1 == l2 && l2 == l3){
    printf("The triangle is equilateral");
  } else if (l1 == l2 || l2 == l3 || l3 == l1){
    printf("The triangle is isosceles");
  } else {
    printf("THe triangle is scalene");
  }
  return 0;
}
